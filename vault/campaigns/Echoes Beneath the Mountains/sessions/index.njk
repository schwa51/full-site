<h1>Session logs</h1>

{% set campaign = "echoes" %}
{% set section  = "sessions" %}

{% set items = (collections.public_content or [])
  | byCampaign(campaign)
  | where("data.section", section)
  | sortBy("data.title") %}

{% set currentUrl = page.url or '' %}

<ul class="list">
  {% for it in items if it.url != currentUrl and it.data.pageSlug != 'index' %}
    <li><a href="{{ it.url }}">{{ it.data.title }}</a></li>
  {% endfor %}
</ul>

