<h1>Session logs</h1>

{% set campaign = campaignSlug or (campaign | slug) %}
{% set section  = type | slug %}

{% set items = (collections.public_content or [])
  | byCampaign(campaign)
  | where("data.section", section)
  | sortBy("data.title") %}

<ul class="list">
  {% for it in items %}
    <li><a href="{{ it.url }}">{{ it.data.title }}</a></li>
  {% endfor %}
</ul>


